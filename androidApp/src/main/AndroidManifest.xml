<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:label="BookHaven"
        android:allowBackup="false"
        android:supportsRtl="true"
        android:icon="@drawable/ic_books_logo"
        android:roundIcon="@drawable/ic_books_logo"
        android:theme="@style/Theme.BookHavenTheme.NoActionBar"
        android:dataExtractionRules="@xml/data_extraction_rules">
        <activity
            android:name="com.jetbrains.kmm.androidApp.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.BookHavenTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>